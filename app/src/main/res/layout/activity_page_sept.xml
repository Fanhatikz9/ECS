<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PageSeptActivity">


    <EditText
        android:id="@+id/commentaire_page_six_deux"
        android:layout_width="345dp"
        android:layout_height="45dp"
        android:inputType="text"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/tv_prime"
        app:layout_constraintTop_toBottomOf="@+id/tv_prime" />

    <EditText
        android:id="@+id/avis"
        android:layout_width="128dp"
        android:layout_height="40dp"
        android:inputType="text"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView4"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/textView4" />

    <EditText
        android:id="@+id/temperature"
        android:layout_width="128dp"
        android:layout_height="40dp"
        android:inputType="text"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/textView2" />


    <EditText
        android:id="@+id/temps"
        android:layout_width="128dp"
        android:layout_height="40dp"
        android:inputType="text"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/textView3" />

    <EditText
        android:id="@+id/meteo"
        android:layout_width="128dp"
        android:layout_height="40dp"
        android:inputType="text"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/textView345"
        app:layout_constraintTop_toTopOf="@+id/textView345" />

    <EditText
        android:id="@+id/heure"
        android:layout_width="128dp"
        android:layout_height="40dp"
        android:inputType="text"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="@+id/textView" />

    <EditText
        android:id="@+id/latitude"
        android:layout_width="128dp"
        android:layout_height="40dp"
        android:inputType="text"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/lat"
        app:layout_constraintTop_toTopOf="@+id/lat" />

    <EditText
        android:id="@+id/longitude"
        android:layout_width="128dp"
        android:layout_height="40dp"
        android:inputType="text"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/longi"
        app:layout_constraintTop_toTopOf="@+id/longi" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="190dp"
        android:layout_height="35dp"
        android:layout_marginTop="24dp"
        android:text="Avis général utilisateur :"
        app:layout_constraintStart_toStartOf="@+id/tv_page_une2"
        app:layout_constraintTop_toBottomOf="@+id/tv_page_une2" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="418dp"
        android:layout_height="225dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_gamma" />

    <TextView
        android:id="@+id/tv_page_une"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="PERFORMANCES ENERGETIQUES"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <TextView
        android:id="@+id/tv_page_une2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="APPRECIATIONS DES USAGERS"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/tv_prime"
        android:layout_width="190dp"
        android:layout_height="35dp"
        android:layout_marginTop="15dp"
        android:text="Commentaire :"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/lat"
        android:layout_width="190dp"
        android:layout_height="35dp"
        android:text="Latitude :"
        app:layout_constraintStart_toStartOf="@+id/commentaire_page_six_deux"
        app:layout_constraintTop_toBottomOf="@+id/commentaire_page_six_deux" />
    <TextView
        android:id="@+id/longi"
        android:layout_width="190dp"
        android:layout_height="35dp"
        android:text="Longitude :"
        app:layout_constraintStart_toStartOf="@+id/lat"
        app:layout_constraintTop_toBottomOf="@+id/lat" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="190dp"
        android:layout_height="35dp"
        android:layout_marginTop="16dp"
        android:text="Température de consigne relevée (°C) :"
        app:layout_constraintStart_toStartOf="@+id/tv_page_une"
        app:layout_constraintTop_toBottomOf="@+id/tv_page_une" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="190dp"
        android:layout_height="35dp"
        android:layout_marginTop="8dp"
        android:text="Temps d'attente de l'eau chaude :"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView345"
        android:layout_width="188dp"
        android:layout_height="36dp"
        android:layout_marginTop="8dp"
        android:text="Méteo lors de la mesure :"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="188dp"
        android:layout_height="36dp"
        android:layout_marginTop="8dp"
        android:text="Heure des mesures :"
        app:layout_constraintStart_toStartOf="@+id/textView345"
        app:layout_constraintTop_toBottomOf="@+id/textView345" />

    <Button
        android:id="@+id/button_last"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="25dp"
        android:text="Suivant"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>